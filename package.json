  },
  "license": "MIT",
  "devDependencies": {
    "@blockone/tslint-config-blockone": "^1.0.0",
    "@types/bunyan": "^1.8.5",
    "@types/jest": "^23.1.4",
    "@types/massive": "^5.4.1",
    "@types/mongodb": "^3.1.4",
    "@types/node": "^10.5.1",
    "@types/node-fetch": "^2.1.6",
    "@types/request-promise-native": "^1.0.15",
    "jest": "^22.4.3",
    "release-it": "^7.5.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "demux": "4.0.1-4ab8dda.0",
